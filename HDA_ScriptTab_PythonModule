def otoyExport():
    import os
    import time
    node = hou.pwd()
    geo = node.geometry()
    
    fPath = hou.hipFile.path()
    fName = len(hou.hipFile.basename())*-1
    fComp = fPath[:fName]
    
        
    
    #data = zzz
    dirTest = os.path.exists(fComp+"/render/")
    if dirTest < 1:
        os.mkdir(fComp+"/render/")
    
    
    frame_range = range(1,10)
    for frame in frame_range:
        hou.setFrame(frame)
        f = open(fComp+"render/zzz"+"_"+str(format(hou.intFrame(),'04'))+".txt", 'w')
        point = geo.points()
        for p in point:
            #print p
            a=p.floatListAttribValue("test")
            for d in a:
                #print d
                ptComp = str(d)
                f.write(ptComp)
                f.write(" ")
